Sample.java

public class Sample {
    public static void main(String[] args) {
        System.out.println("Hello, World!");
    }
}


dockerfile

FROM openjdk
WORKDIR /app
COPY . /app
RUN ["javac","Sample.java"]
CMD ["java","Sample"]


creating image- docker build -t dipapp .

creating containers for the image- docker run --name dipcon1 dipapp




PYTHON

creating image- docker build -t dippy .


creating containers for the image- docker run --name dippy1 dippy


docker tag 9f060979e966 samarthasm01/mobapp:1.1

docker push docker.io/samarthasm01/mobapp:1.1

docker pull anilkumarbh/mobapp:1.1

docker run --name anil1 anilkumarbh/mobapp:1.1
